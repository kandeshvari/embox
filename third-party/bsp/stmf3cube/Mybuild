@BuildDepends(core)
static module cmsis extends embox.arch.arm.cmsis {
	source "cmsis_impl.h"
	depends core
}


@Build(stage=1,script="true")
@BuildDepends(core)
static module system_init {
	@DefineMacro("STM32F303xC")
	@DefineMacro("USE_RTOS=0")
	@DefineMacro("USE_STDPERIPH_DRIVER")
	@IncludePath("$(EXTERNAL_BUILD_DIR)/third_party/bsp/stmf3cube/core/STM32Cube_FW_F3_V1.3.0/Drivers/STM32F3xx_HAL_Driver/Inc")
	@IncludePath("$(EXTERNAL_BUILD_DIR)/third_party/bsp/stmf3cube/core/STM32Cube_FW_F3_V1.3.0/Drivers/CMSIS/Device/ST/STM32F3xx/Include")
	@IncludePath("$(EXTERNAL_BUILD_DIR)/third_party/bsp/stmf3cube/core/STM32Cube_FW_F3_V1.3.0/Drivers/CMSIS/Include")
	@IncludePath("$(EXTERNAL_BUILD_DIR)/third_party/bsp/stmf3cube/core/STM32Cube_FW_F3_V1.3.0/Projects/STM32F3-Discovery/Examples/BSP/Inc")
	@AddPrefix("^BUILD/extbld/third_party/bsp/stmf3cube/core")
	source "./STM32Cube_FW_F3_V1.3.0/Projects/STM32F3-Discovery/Examples/BSP/Src/system_stm32f3xx.c"
}


@Build(stage=1,script="true")
@BuildDepends(core)
module arch extends embox.arch.arch {
	source "arch.c"

	depends system_init
}

