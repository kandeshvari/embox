package third_party.bsp.stmf3cube

@Build(stage=1,script="$(EXTERNAL_MAKE) download extract patch")
@BuildArtifactPath(cppflags="-DSTM32F303xC -DUSE_RTOS=0 -I$(ROOT_DIR)/third-party/bsp/stmf3cube/ $(addprefix -I$(EXTERNAL_BUILD_DIR)/third_party/bsp/stmf3cube/core/STM32Cube_FW_F3_V1.3.0/,Drivers/STM32F3xx_HAL_Driver/Inc Drivers/CMSIS/Device/ST/STM32F3xx/Include Drivers/CMSIS/Include Projects/STM32F3-Discovery/Examples/BSP/Inc Drivers/BSP/STM32F3-Discovery)")
static module core extends third_party.bsp.st_bsp_api {

	option number hse_freq_hz = 8000000 /* STM32F3Discovery oscillator */

	@Cflags("-Wno-unused")
